{
  "author": "eternal",
  "description": "Shows the dislike button that hides items on the map on HAR on before navigation.",
  "name": "HAR Dislike Button",
  "version": "1.0.0",
  "content_scripts": [
    {
      "js": [
        "src/main.js"
      ],
      "matches": [
        "*://*.har.com/*"
      ]
    }
  ],
  "icons": {
    "16": "icons/16.png",
    "19": "icons/19.png",
    "32": "icons/32.png",
    "38": "icons/38.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "96": "icons/96.png",
    "128": "icons/128.png",
    "256": "icons/256.png",
    "512": "icons/512.png"
  },
  "permissions": [],
  "host_permissions": [
    "*://*/*"
  ],
  "manifest_version": 3
}